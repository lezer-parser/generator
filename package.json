{
  "name": "@lezer/generator",
  "version": "1.5.1",
  "description": "Parser generator for the incremental lezer parser",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./test": {
      "import": "./dist/test.js",
      "require": "./dist/test.cjs"
    },
    "./dist/test": {
      "import": "./dist/test.js",
      "require": "./dist/test.cjs"
    },
    "./rollup": {
      "import": {
        "types": "./dist/unplugin.d.ts",
        "default": "./dist/unplugin.js"
      },
      "require": {
        "types": "./dist/unplugin.d.cts",
        "default": "./dist/unplugin.cjs"
      }
    },
    "./dist/unplugin": {
      "import": {
        "types": "./dist/unplugin.d.ts",
        "default": "./dist/unplugin.js"
      },
      "require": {
        "types": "./dist/unplugin.d.cts",
        "default": "./dist/unplugin.cjs"
      }
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Marijn Haverbeke <marijn@haverbeke.berlin>",
  "license": "MIT",
  "devDependencies": {
    "@lezer/lr": "^1.3.14",
    "@marijn/buildtool": "^0.1.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^20.5.0",
    "ist": "^1.1.1",
    "mocha": "^10.2.0",
    "tsup": "^7.2.0",
    "unplugin": "^1.5.0"
  },
  "dependencies": {
    "@lezer/common": "^1.1.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lezer-parser/generator.git"
  },
  "scripts": {
    "watch": "node build.js --watch",
    "prepare": "node build.js",
    "test": "mocha"
  },
  "bin": {
    "lezer-generator": "./src/lezer-generator.cjs"
  }
}
